# Example Configuration
# Copy this to .env and update with your values

# Path to your Flutter app's SQLite database
# Common locations:
# - Android: data/data/com.example.findar/databases/listings.db
# - Development: In your app's documents directory
DATABASE_PATH=../android/app/src/main/assets/databases/listings.db

# Firebase service account key file
FIREBASE_KEY_PATH=firebase-service-account.json

# Server configuration
PORT=5000
HOST=0.0.0.0
DEBUG=True

# Scheduler settings
CHECK_INTERVAL_HOURS=1
